<div class="flex justify-end items-center h-16 bg-gray-800 shadow">
    <a class="text-grey-300  hover:text-white px-3 py-2 rounded-md text-sm font-medium" href="/">Home</a>
    <a class="text-grey-300  hover:text-white px-3 py-2 rounded-md text-sm font-medium" href="/link">Links</a>
    {% block  logged%}
        {% if app.user %}
            <a class="text-grey-300  hover:text-white px-3 py-2 rounded-md text-sm font-medium">
                Logout - Hello {{ app.user.userIdentifier }}
            </a>
        {% else %}
            <a class="text-grey-300  hover:text-white px-3 py-2 rounded-md text-sm font-medium">
                Login
            </a>
        {% endif %}
    {% endblock %}
</div>